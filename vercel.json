{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/api/extract"
    }
  ],
  "functions": {
    "api/*.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}

